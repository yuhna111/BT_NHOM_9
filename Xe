public abstract class Xe {
    private String bienSo;
    private String loaiXe;
    private long thoiGianVao;
    private long thoiGianRa;

    public Xe(String bienSo, String loaiXe, long thoiGianVao) {
        this.bienSo = bienSo;
        this.loaiXe = loaiXe;
        this.thoiGianVao = thoiGianVao;
    }
    public String getBienSo() {
        return bienSo;
    }
    public String getLoaiXe() {
        return loaiXe;
    }
    public long getThoiGianVao() {
        return thoiGianVao;
    }
    public long getThoiGianRa() {
        return thoiGianRa;
    }
    public void setThoiGianRa(long thoiGianRa) {
        this.thoiGianRa = thoiGianRa;
    }
    public abstract double tinhPhi();

    @Override
    public String toString() {
        return "Bien so: " + bienSo + ", Loai xe: " + loaiXe;
    }
}
